{{#partial "contenido"}}
    <div class="form-container">
        <form action="/cargar-csv" method="POST" enctype="multipart/form-data">
            <h1>Cargar Archivo CSV</h1>
            <br>
            <h4>FORMATO: TIPODOC;NRODOC;NOMBRE;APELLIDO;EMAIL;FECHANAC;TIPOCOL;CANTCOL</h4>
            <h5>RECUERDE QUE LOS ARCHIVOS SUBIDOS SON ENTERAMENTE DE SU RESPONSABILIDAD</h5>
            <input type="file" id="csvFile" name="csvFile" accept=".csv" required>
            <button type="submit">Cargar</button>
        </form>
    </div>
    {{/partial}}
{{> templates/base title="Carga masiva"}}